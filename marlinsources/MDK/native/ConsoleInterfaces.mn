// Native console interfaces for Marlin
module std;

internal static class ConsoleInterfaces
{
    public static void PutCharacter(char c)
    {
        @[("c_putchar", @[("unbox", "std::Character", 'a')] )];
    }
    
    public static char GetCharacter()
    {
        return @[("box", "std::Character", @[("c_getchar")])];
    }
}